{
  "name": "the-simplifier",
  "description": "A simple package to simplify your tasks as a programmer",
  "version": "1.0.0",
  "main": "dist/simplifier.js",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/gbrasil3g/the-simplifier/issues"
  },
  "homepage": "https://github.com/gbrasil3g/the-simplifier/blob/master/docs/README.md",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/gbrasil3g/the-simplifier"
  },
  "scripts": {
    "start:dev": "ts-node-dev 'src/index.ts'",
    "build": "bili",
    "test": "yarn test:types && yarn test:unit",
    "test:unit": "jest",
    "test:types": "tsc -p ./tsconfig.build.json --noEmit",
    "lint": "eslint ./src ./tests --ext .ts",
    "lint:fix": "eslint ./src ./tests --ext .ts --fix",
    "style:check": "prettier --check 'src/**/*.ts' 'src/*.ts' 'tests/*.ts'",
    "style:fix": "prettier --write 'src/**/*.ts' 'src/*.ts' 'tests/*.ts'",
    "prepublishOnly": "yarn test && yarn build"
  },
  "devDependencies": {
    "@babel/core": "^7.12.10",
    "@babel/preset-env": "^7.12.11",
    "@babel/preset-typescript": "^7.12.7",
    "@types/color-convert": "^2.0.0",
    "@types/hex-color-regex": "^1.1.0",
    "@types/jest": "^26.0.20",
    "@types/node": "^14.14.21",
    "@types/valid-url": "^1.0.3",
    "@typescript-eslint/eslint-plugin": "^4.12.0",
    "@typescript-eslint/parser": "^4.13.0",
    "ava": "^3.15.0",
    "babel-jest": "^26.6.3",
    "bili": "^5.0.5",
    "eslint": "^7.17.0",
    "jest": "^26.6.3",
    "prettier": "^2.2.1",
    "rollup-plugin-typescript2": "^0.29.0",
    "ts-node": "^9.1.1",
    "ts-node-dev": "^1.1.1",
    "typescript": "^4.1.3"
  },
  "dependencies": {
    "chalk": "^4.1.0",
    "color-convert": "^2.0.1",
    "debug": "^4.3.1",
    "hex-color-regex": "^1.1.0",
    "is-hexcolor": "^1.0.0",
    "isurl": "^4.0.1",
    "moment": "^2.29.1",
    "tesseract.js": "^2.1.4",
    "valid-url": "^1.0.9",
    "winston": "^3.3.3",
    "words-binary": "^1.1.2"
  }
}
